{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Sudo Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "sudo": true
        },
        {
            "name": "Gunicorn: Run App",
            "type": "python",
            "request": "launch",
            "program": "gunicorn",
            "args": [
                "-b",
                "0.0.0.0:8080",
                "src.app:main()"
            ],
            "console": "integratedTerminal"//,
            //"pythonPath": "${workspaceFolder}/../.venv495/bin/python"
        }
    ]
}